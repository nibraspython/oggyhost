{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/img", "dest": "/api/img.js" },
    { "src": "/api/ss", "dest": "/api/screenshot.js" },
    { "src": "/", "dest": "/index.js" }
  ]
}
