{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/img", "dest": "/api/img.js" },
    { "src": "/insta", "dest": "/api/insta.js" },
    { "src": "/ss", "dest": "/api/ss.js" },
    { "src": "/ogg", "dest": "/api/image.js" },
    { "src": "/", "dest": "/index.js" }
  ]
}
