{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/img", "dest": "/api/img" },
    { "src": "insta", "dest": "/api/insta" },
    { "src": "/api/ss", "dest": "/api/ss" },
    { "src": "/", "dest": "/api/index" }
  ],
  "functions": {
    "api/insta": {
      "runtime": "@vercel/node",
      "memory": 1024,
      "maxDuration": 15
    }
  }
}
