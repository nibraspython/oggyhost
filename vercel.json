{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/img", "dest": "/api/img" },
    { "src": "/api/insta", "dest": "/api/insta" },
    { "src": "/api/ss", "dest": "/api/ss" },
    { "src": "/api/image", "dest": "/api/image" },
    { "src": "/api/.*", "dest": "/api/index" }
  ]
}
